{
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@googlemaps/react-wrapper": "^1.2.0",
    "@turf/turf": "^7.2.0",
    "babylonjs": "^8.13.0",
    "babylonjs-loaders": "^8.13.0",
    "caniuse-lite": "^1.0.30001726",
    "chart.js": "^4.5.0",
    "dxf-parser": "^1.1.2",
    "financial": "^0.2.4",
    "geojson": "^0.5.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "papaparse": "^5.5.3",
    "react-chartjs-2": "^5.3.0",
    "three": "^0.177.0",
    "xlsx": "^0.18.5",
    "@babylonjs/core": "^6.32.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/puppeteer": "^4.10.2",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "axe-core": "^4.10.3",
    "axe-playwright": "^2.1.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^30.0.3",
    "playwright": "^1.53.1",
    "puppeteer": "^22.15.0",
    "ts-jest": "^29.4.0",
    "wcag-contrast": "^3.0.0",
    "prettier": "^3.2.5",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.3",
    "depcheck": "^1.4.3",
    "tailwindcss": "^3.4.16",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "vitest": "^0.34.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "jsdom": "^22.1.0",
    "vitest-axe": "^0.1.0",
    "vite-plugin-pwa": "^0.16.4",
    "@vitejs/plugin-react": "^4.2.0",
    "glob": "^10.3.10"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "typecheck": "tsc --build --pretty",
    "prepare": "husky",
    "build:tailwind:static": "tailwindcss -c tailwind.config.cjs -i ./css/input.css -o ./css/tailwind-static.css --minify",
    "depcheck": "depcheck --ignores='@types/*,husky,@googlemaps/js-api-loader,@googlemaps/react-wrapper,@turf/turf,babylonjs,babylonjs-loaders,@babylonjs/core,caniuse-lite,chart.js,dxf-parser,financial,geojson,jspdf,jspdf-autotable,papaparse,react-chartjs-2,three,xlsx,axe-playwright,ts-jest,vitest-axe,vite-plugin-pwa'",
    "browserslist:update": "bunx update-browserslist-db@latest --update-db",
    "fix:tailwind-html": "node scripts/update-static-tailwind.js",
    "fix:csp": "node scripts/update-static-csp.js",
    "fix:imports": "node scripts/codemod-import-alias.js",
    "build": "bun run build:tailwind:static && node build.js",
    "build:prod": "NODE_ENV=production bun run build:tailwind:static && NODE_ENV=production node build.js",
    "dev": "cd dist && npx http-server -p 8080 --cors",
    "serve": "bun run build && bun run dev",
    "test": "bun run test:unit && node test/run-all-tests.js",
    "test:unit": "vitest run",
    "test:accessibility": "bun run test:a11y && bun run test:a11y:components && bun run test:a11y:audit",
    "test:performance": "bun run test:perf:color-palette && bun run test:css-performance",
    "test:cross-browser": "bun run test:integration",
    "test:a11y": "node test/axe-tests/axe-color-palette.js",
    "test:a11y:components": "node test/axe-tests/axe-calculator.js",
    "test:a11y:audit": "node test/accessibility/accessibility-audit.js",
    "test:perf:color-palette": "node test/performance/color-palette-performance.js",
    "test:integration": "node test/integration/cross-browser-test.js",
    "test:css-performance": "node test/performance-optimization/css-performance-analysis.js",
    "test:consistency": "node test/consistency/color-consistency-check.js",
    "validate": "bun run validate:build && bun run validate:assets",
    "validate:build": "test -f dist/index.html && test -f dist/calculator-gamified.html && test -f dist/manifest.json",
    "validate:assets": "test -d dist/css && test -d dist/js && test -d dist/img",
    "clean": "rm -rf dist/*",
    "deploy:local": "bun run build && bun run dev",
    "deploy:gh-pages": "bun run build:prod && echo 'Build ready for GitHub Pages deployment'"
  },
  "name": "beyond-solutions-landing",
  "version": "1.9.0",
  "description": "Landing page for Beyond Solutions, a platform designed to strategically integrate and manage all elements that make up a world-class real estate development.",
  "main": "build.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Smartports/beyond-solutions-landing.git"
  },
  "keywords": [
    "real-estate",
    "development",
    "calculator",
    "gamification",
    "pwa",
    "maps",
    "3d-visualization"
  ],
  "author": "Beyond Solutions Team",
  "license": "ISC",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
